   "source": [
    "a = int(input('enter a number to check prime or not : '))\n",
    "\n",
    "if a < 2 :\n",
    "    print('not a prime')\n",
    "\n",
    "else:\n",
    "    for i in range(2,a):\n",
    "        if a % i == 0:\n",
    "            print('not prime')\n",
    "            break\n",
    "    else :\n",
    "        print(' prime')"
